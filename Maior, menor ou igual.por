programa {
  funcao inicio() {
    
  //Declaração da variáveis.
  
  inteiro valor


  //Solicitação de dados para o usuário.

  escreva("Digite um valor: ")
  leia(valor)


  //Exibindo resultado para o usuário.

  se(valor > 10){
  escreva("É MAIOR QUE 10")
  }senao se(valor < 10){
  escreva("É MENOR QUE 10")
  }senao{ valor=10
  escreva("VALOR É IGUAL A 10")
  }
  }
}
